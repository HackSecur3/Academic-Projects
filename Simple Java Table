import java.util.*;

public class SpecialMultTable {
  
  
  public static void main(String[] args) { 
  Scanner console = new Scanner(System.in);
    int bn, fn, ln, inc, ms;
    bn = console.nextInt();
    fn = console.nextInt(); 
    ln = console.nextInt();
    inc = console.nextInt();
    ms = console.nextInt();
    System.out.println("UTSA - Fall 2021 - CS 1083 - 002 - Prj 1 -written by <Navtej Dharmapuri Thirumala> - <pez776>");
    System.out.println();
    System.out.println("Input base number (bn): " + bn);
    System.out.println("Input first number (fn): " + fn);
    System.out.println("Input last number (ln): " + ln);
    System.out.println("Input the increasement (inc); " + inc);
    System.out.println("Input multiple(s) to skip (ms): " + ms);
    System.out.println();
    System.out.println("-Special Multiplication Table-");
    System.out.println();
    System.out.println("Value" + "   " + "Result");
    for( int i = fn; i < ln;i+=inc){
    int sum = 0;
      sum = i;
      System.out.print(sum);
      int result = i * bn;
      if((result)%ms == 0){
      System.out.print("  -> Skipped - " + result + " <-");
      System.out.println();
      }
      else {
      System.out.print("      " + result);
      System.out.println();
      }
    }
    System.out.println("Thank you for using this software, <Navtej Dharmapuri Thirumala>.");
    
  }  
}
